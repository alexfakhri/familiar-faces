<p><%= @article.title %></p>
<p><%= @article.story %></p>
<p><%= @article.location %></p>
<%= image_tag @article.photo.url(:portrait) %>

Tags: <%= raw @article.tag_list.map { |t| link_to t, tag_path(t) }.join(', ') %>

<%= link_to "Edit Story", edit_article_path(@article) %>
<%= link_to "Delete Story", article_path(@article), method: :delete %>
