<%= form_for [@user, @article], html: {multipart: true} do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :story %>
  <%= f.text_area :story %>
  <%= f.label :tag_list, "Tags (separated by commas)" %><br />
  <%= f.text_field :tag_list %>
  <%= f.file_field :photo %>
  <%= f.submit "Add Story" %>
<% end %>
